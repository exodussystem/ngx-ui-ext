{
  "name": "ngx-ui-ext",
  "private": false,
  "version": "1.1.6",
  "description": "UI Extension of Angular 2+",
  "repository": {},
  "main": "bundles/ngx-ui-ext.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typedoc": "typedoc",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --port 8000 --config webpack.example.js --history-api-fallback --content-base example",
    "build": "npm run clean && tsc && tsc -m es6 && cross-env NODE_ENV=production webpack",
    "clean": "shx rm -rf dist doc",
    "package": "npm run build"
  },
  "keywords": [
    "angular",
    "bootstap",
    "ng",
    "ng2",
    "ngx",
    "ui",
    "angular2",
    "angular4",
    "ngx-ui-ext",
    "twitter-bootstrap"
  ],
  "author": "Tuyen Tran <tuyen.tran@exodussystem.com>",
  "license": "MIT",
  "homepage": "https://github.com/exodussystem/ngx-ui-ext",
  "dependencies": {
    "@angular/animations": "^4.3.3",
    "@angular/common": "^4.3.3",
    "@angular/compiler": "^4.3.3",
    "@angular/compiler-cli": "^4.3.3",
    "@angular/core": "^4.3.3",
    "@angular/forms": "^4.3.3",
    "@angular/http": "^4.3.3",
    "@angular/platform-browser": "^4.3.3",
    "@angular/platform-browser-dynamic": "^4.3.3",
    "@angular/platform-server": "^4.3.3",
    "@angular/router": "^4.3.3",
    "angular-in-memory-web-api": "~0.2.2",
    "core-js": "^2.4.1",
    "jquery": "^3.1.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.3",
    "spin.js": "^2.3.2",
    "typescript": "^2.3.2"
  },
  "devDependencies": {
    "@angular/compiler": "^4.3.3",
    "@angular/compiler-cli": "^4.3.3",
    "@types/jquery": "^2.0.41",
    "@types/spin.js": "^2.3.0",
    "awesome-typescript-loader": "~3.2.3",
    "css-loader": "^0.26.1",
    "style-loader": "^0.13.1",
    "concurrently": "^3.1.0",
    "cross-env": "^4.0.0",
    "shelljs": "^0.7.0",
    "shx": "^0.1.4",
    "lite-server": "^2.2.2",
    "ts-helpers": "^1.1.2",
    "ts-node": "^2.0.0",
    "tslib": "^1.5.0",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.3.2",
    "typedoc": "0.5.3",
    "webpack": "^2.5.0",
    "webpack-dev-server": "2.4.1",
    "zone.js": "^0.8.10"
  }
}